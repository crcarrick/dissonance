  "dependencies": {
    "apollo-datasource": "^0.7.2",
    "apollo-server": "^2.17.0",
    "apollo-server-caching": "^0.5.2",
    "aws-sdk": "^2.759.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "camelcase-keys": "^6.2.2",
    "dataloader": "^2.0.0",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
