    "graphql-tools": "^6.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "knex-migrate": "^1.7.4",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.27",
    "pg": "^8.3.3",
    "sequelize": "^6.3.5",
    "snake-case": "^3.0.3",
    "socket.io": "^2.3.0",
    "uuid": "^8.3.0"
